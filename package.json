{
  "name": "xtreamly_trader",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "version": "0.0.1",
  "scripts": {
    "vGetReserveDataUSDC": "npx ts-node scripts/arbitrum/sepolia/viewFunctions/getReserveData.ts --token 0x75faf114eafb1BDbe2F0316DF893fd58CE46AA4d",
    "wDepositETH": "npx ts-node scripts/arbitrum/sepolia/ethInteractor/depositETH.ts --amount 0.001",
    "account": "hardhat run scripts/listAccount.ts",
    "chain": "hardhat node --network hardhat --no-deploy",
    "compile": "hardhat compile",
    "ncompile": "nodemon --exec 'yarn compile' --watch . --ext js,sol,ts",
    "nTest": "nodemon --exec 'npx hh test --vvvv test/aaveV3/supply_via_xtr.ts --network hardhat' --watch . --ext js,sol,ts",
    "arbSupplyTest": "nodemon --exec 'npx hh test --vvvv test/arbitrum_tests/supply_arbi_aaveV3.ts --network hardhat' --watch . --ext js,sol,ts",
    "deploy": "hardhat deploy",
    "fork": "MAINNET_FORKING_ENABLED=true hardhat node --network hardhat --no-deploy",
    "generate": "hardhat run scripts/generateAccount.ts",
    "flatten": "hardhat flatten",
    "lint": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore ./*.ts ./deploy/**/*.ts ./scripts/**/*.ts ./test/**/*.ts",
    "lint-staged": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore",
    "format": "prettier --write ./*.ts ./deploy/**/*.ts ./scripts/**/*.ts ./test/**/*.ts",
    "test": "REPORT_GAS=true hardhat test --network hardhat",
    "verify": "hardhat etherscan-verify",
    "hardhat-verify": "hardhat verify",
    "testInteractor": "yarn hardhat run scripts/forkInteractoor.ts --network hardhat",
    "start": "ts-node -r tsconfig-paths/register strategy.ts",
    "clean": "rm -rf ./cache ./artifacts ./typechain-types",
    "build": "rm -rf dist && tsc && tsc-alias"
  },
  "devDependencies": {
    "@balancer-labs/v2-interfaces": "^0.4.0",
    "@defisaver/automation-sdk": "3.0.2",
    "@defisaver/sdk": "1.0.14",
    "@defisaver/tokens": "1.5.10",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.7",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-verify": "^2.0.10",
    "@typechain/ethers-v5": "^11.1.2",
    "@typechain/hardhat": "^9.1.0",
    "@types/eslint": "^8",
    "@types/mocha": "^10.0.8",
    "@types/prettier": "^2",
    "@types/qrcode": "^1",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.19.0",
    "chai": "^4.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethers": "^6.13.2",
    "hardhat": "^2.22.10",
    "hardhat-deploy": "^0.12.4",
    "hardhat-deploy-ethers": "^0.4.2",
    "hardhat-gas-reporter": "^2.2.1",
    "hardhat-tracer": "^3.1.0",
    "prettier": "^2.8.4",
    "solidity-coverage": "^0.8.13",
    "solmate": "^6.8.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^5.1.6",
    "viem": "^2.21.55",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@bgd-labs/aave-address-book": "^4.2.1",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.1.0",
    "@typechain/ethers-v6": "^0.5.1",
    "dotenv": "^16.0.3",
    "envfile": "^6.18.0",
    "qrcode": "^1.5.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  }
}
